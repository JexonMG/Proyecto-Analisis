// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id           Int      @id @default(autoincrement())
  username     String   @unique
  password     String
  role         String
  name         String
  email        String   @unique
  profileImage String?
  lastLogin    DateTime
}
